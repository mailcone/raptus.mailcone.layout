<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
        "http://www.w3.org/TR/html4/strict.dtd"><html>
  <head>
    <title>Mailcone</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon"
          type="image/x-icon"
          href="favicon.ico"
          tal:attributes="href static.get('favicon.ico')"/>
  </head>
  <body>
    <div id="wrapper">
      <div id="container">
        <div id="header">
          <!--<div class="cell position-0 width-16">-->
            <div tal:replace="structure provider:header" />
          <!--</div> -->
        </div>
        
      <div id="middle-wrap">
              <div id="navigation" class="box">
                  <div tal:replace="structure provider:navigation" />
              </div>
           
            
             <div id="content-wrap" class="box">
                  <div id="content-before">
                    <div tal:replace="structure provider:content.before" />
                  </div>
                  <div id="content">
                    <div tal:replace="structure view.content()" />
                  </div>
                  <div id="content-after">
                    <div tal:replace="structure provider:content.after" />
                  </div>
              </div>
        </div>
        
        <div id="footer">
          <div tal:replace="structure provider:footer" />
        </div>
      </div>
    </div>
  </body>
</html>


