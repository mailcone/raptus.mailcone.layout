<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>Mailcone</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  </head>
  <body>
    <div id="wrapper">
      <div id="container">
        <div id="header" class="row">
          <div class="cell position-0 width-16">
            <div tal:replace="structure provider:header" />
          </div>
        </div>
        <div id="columns" class="row">
            <div class="cell position-4 width-12">
              <div id="content-before">
                <div tal:replace="structure provider:content.before" />
              </div>
              <div id="content">
                <div tal:replace="structure view.content()" />
              </div>
              <div id="content-after">
                <div tal:replace="structure provider:content.after" />
              </div>
            </div>
            <div class="cell position-0 width-4">
              <div id="navigation">
                <div id="navigation-body">
                  <div tal:replace="structure provider:navigation" />
                </div>
                <div id="navigation-footer">
                </div>
              </div>
            </div>
        </div>
        <div id="footer">
          <div tal:replace="structure provider:footer" />
        </div>
      </div>
    </div>
  </body>
</html>


