<table class="ui-datatable display"
       tal:attributes="id view.id;
                       data-ajaxurl view.ajaxurl;
                       data-tabletools view.tabletools;">
    <thead>
        <tr>
            <th tal:repeat="col view.columns" tal:content="col"/>
            <th width="20px" class="ui-table-action" tal:repeat="col view.actions" tal:content="col.get('title')"/>
        </tr>
    </thead>
    <tbody>
        
    </tbody>
</table>
